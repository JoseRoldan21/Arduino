const int led0 = 5;
//************************************* SETUP *************************************
void setup()              
{
  pinMode(led0, OUTPUT);    
  for (int i=0; i<30; i++){
 digitalWrite (led0 ,HIGH);
 delay(100);
 digitalWrite (led0, LOW);
 delay (100); 
  }
}

